    "type": "library",
    "license": "AGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "WindSearch\\Index\\": "src/",
            "WindSearch\\Core\\": "src/lib/",
            "WindSearch\\DAO\\": "src/DAO/",
            "WindSearch\\Exceptions\\": "src/Exceptions/"
        }
    },
    "authors": [
        {
            "name": "rock365"
        }
    ],
    "require": {
        "php": ">=7.3"
    }
}